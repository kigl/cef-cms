{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "path",
            "url": "packagest/core"
        },
        {
            "type": "path",
            "url": "packagest/backend"
        },
        {
            "type": "path",
            "url": "packagest/comment"
        },
        {
            "type": "path",
            "url": "packagest/shop"
        },
        {
            "type": "path",
            "url": "packagest/infosystem"
        },
        {
            "type": "path",
            "url": "packagest/page"
        },
        {
            "type": "path",
            "url": "packagest/tag"
        },
        {
            "type": "path",
            "url": "packagest/service"
        },
        {
            "type": "path",
            "url": "packagest/user"
        }
    ],
    "require": {
        "kigl/cef-core": "^1.0",
        "kigl/cef-module-user": "^1.0",
        "kigl/cef-module-comment": "*",
        "kigl/cef-module-infosystem": "*",
        "kigl/cef-module-page": "*",
        "kigl/cef-module-tag": "*",
        "kigl/cef-module-shop": "*",
        "kigl/cef-module-service": "^1.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
